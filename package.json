{
    "private": true,
    "packageManager": "pnpm@7.0.0",
    "scripts": {
        "build": "vue-tsc --noEmit && cross-env NODE_OPTIONS=--max_old_space_size=8192 vite build",
        "build-test": "vue-tsc --noEmit && cross-env NODE_OPTIONS=--max_old_space_size=8192 vite build --mode test",
        "build-uat": "vue-tsc --noEmit && cross-env NODE_OPTIONS=--max_old_space_size=8192 vite build --mode uat",
        "build-theme": "vue-tsc --noEmit && cross-env NODE_OPTIONS=--max_old_space_size=8192 vite build --mode theme",
        "dev": "cross-env vite --open",
        "dev-local": "cross-env LOCAL=1 vite --open",
        "lint-quiet": "eslint . --fix --quiet",
        "lint": "eslint . --fix",
        "typecheck": "vue-tsc --noEmit",
        "preview": "vite preview",
        "test": "vitest",
        "webstorm-integration": "vitest --watch --reporter=dot --reporter=json --outputFile=.vitest-result.json",
        "prepare": "husky install",
        "deploy-test": "tsx scripts/deploy-test.ts",
        "gen-icons": "tsx scripts/genIcons.ts"
    },
    "dependencies": {
        "@aws-sdk/abort-controller": "^3.310.0",
        "@aws-sdk/client-s3": "^3.328.0",
        "@aws-sdk/lib-storage": "^3.328.0",
        "@aws-sdk/s3-request-presigner": "^3.328.0",
        "@aws-sdk/xhr-http-handler": "^3.310.0",
        "@vuelidate/core": "^2.0.2",
        "@vuelidate/validators": "^2.0.2",
        "@vueuse/core": "^10.1.2",
        "@vueuse/head": "^1.1.26",
        "axios": "^1.4.0",
        "binconv": "^0.2.0",
        "camelcase": "^7.0.1",
        "dayjs": "^1.11.7",
        "decamelize": "^6.0.0",
        "html2canvas": "^1.4.1",
        "html2pdf.js": "^0.10.1",
        "js-cookie": "^3.0.5",
        "jspdf": "^2.5.1",
        "navuelidate": "latest",
        "pinia": "^2.0.35",
        "qiankun": "^2.10.6",
        "v-demi-request": "latest",
        "vue": "^3.2.47",
        "vue-i18n": "^9.2.2",
        "vue-router": "^4.1.6",
        "vue3-uuid": "^1.0.0",
        "xe-utils": "^3.5.7"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^0.38.5",
        "@iconify-json/carbon": "^1.1.16",
        "@intlify/unplugin-vue-i18n": "0.10.0",
        "@jridgewell/sourcemap-codec": "^1.4.15",
        "@types/js-cookie": "^3.0.3",
        "@types/node": "^18.16.0",
        "@unocss/reset": "^0.51.12",
        "@vitejs/plugin-legacy": "^4.0.3",
        "@vitejs/plugin-vue": "^4.2.1",
        "@vitejs/plugin-vue-jsx": "^3.0.1",
        "@vue/test-utils": "latest",
        "cross-env": "^7.0.3",
        "eslint": "8.39.0",
        "eslint-plugin-jsonc": "^2.7.0",
        "github-markdown-css": "^5.2.0",
        "husky": "^8.0.3",
        "jsdom": "^22.0.0",
        "naive-ui": "^2.34.3",
        "prettier": "^2.8.8",
        "rollup": "3.18.0",
        "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.4.15",
        "terser": "^5.17.1",
        "ts-node": "^10.9.1",
        "tsx": "^3.12.7",
        "typescript": "^5.0.4",
        "unocss": "^0.51.12",
        "unplugin-auto-import": "^0.15.3",
        "unplugin-icons": "^0.16.1",
        "unplugin-vue-components": "^0.24.1",
        "vfonts": "^0.0.3",
        "vite": "^4.3.5",
        "vite-plugin-node-polyfills": "^0.8.2",
        "vite-plugin-pages": "^0.29.0",
        "vite-plugin-vue-layouts": "^0.8.0",
        "vite-plugin-vue-markdown": "^0.23.0",
        "vite-plugin-vue-setup-extend": "^0.4.0",
        "vite-svg-loader": "^4.0.0",
        "vitest": "^0.31.0",
        "vue-tsc": "1.2.0"
    },
    "pnpm": {
        "overrides": {
            "navuelidate": "latest",
            "v-demi-request": "latest",
            "typescript": "5.0.4"
        }
    },
    "eslintConfig": {
        "extends": "@antfu"
    },
    "volta": {
        "node": "18.16.0"
    }
}
